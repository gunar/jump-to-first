{
    "manifest_version": 2,
    "content_scripts": [ {
        "exclude_globs":    [  ],
        "include_globs":    [ "*" ],
        "js":               [ "jumpToFirst.user.js" ],
        "matches":          [
          "https://workflowy.com/*",
          "http://workflowy.com/*"
        ],
        "run_at": "document_end"
    } ],
    "converted_from_user_script": true,
    "description":  "User Ctrl+Enter to jump from Workflowy's search page into the first match.",
    "name":         "Workflowy: Jump to first match",
    "version":      "1",
    "icons": { "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png" }
}
